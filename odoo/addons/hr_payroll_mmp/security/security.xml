<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<data noupdate="1">

        <record id="seq_work_type" model="ir.sequence">
            <field name="name">Work Type</field>
            <field name="code">hr.work.type</field>
            <field name="prefix">C</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">3</field>
        </record>

        <record id="seq_hr_work_type" model="ir.sequence">
            <field name="name">Work Entry</field>
            <field name="code">hr.work.entry</field>
            <field name="prefix">WE/%(y)s/%(month)s/</field>
            <field name="use_date_range">True</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">3</field>
        </record>

        <record id="seq_hr_bonus" model="ir.sequence">
            <field name="name">Bonus</field>
            <field name="code">hr.bonus</field>
            <field name="prefix">BNS/%(y)s/%(month)s/</field>
            <field name="use_date_range">True</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>